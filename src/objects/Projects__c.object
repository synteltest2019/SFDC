    <fields>
        <fullName>Duration_In_Business_Days__c</fullName>
        <externalId>false</externalId>
        <formula>IF(!(ISNULL(Planned_Start_Date__c) &amp;&amp; !(ISNULL(Planned_End_Date__c))) ,
((Planned_End_Date__c) - ( Planned_Start_Date__c )) -
( FLOOR ( ( ( ((Planned_End_Date__c)) - (Planned_Start_Date__c)) / 7 ) ) * 2 ) +
CASE(MOD ( (Planned_Start_Date__c) - DATE( 1900,1,6 ),7),
0,CASE( MOD (((Planned_End_Date__c)) - DATE( 1900,1,6 ),7),0,1,2 ),
1,CASE( MOD (((Planned_End_Date__c)) - DATE( 1900,1,6 ),7),0,2,1 ),
IF(MOD((Planned_Start_Date__c) - DATE( 1900,1,6 ),7) - MOD(((Planned_End_Date__c)) - DATE( 1900,1,6 ),7) &lt;= 0 ,0,
IF(MOD(((Planned_End_Date__c)) - DATE( 1900,1,6 ),7) = 0,1,-2 ) ) )
,
((TODAY()) - ( Planned_Start_Date__c )) -
( FLOOR ( ( ( (TODAY()) - (Planned_Start_Date__c)) / 7 ) ) * 2 ) +
CASE(MOD ( (Planned_Start_Date__c) - DATE( 1900,1,6 ),7),
0,CASE( MOD ((TODAY()) - DATE( 1900,1,6 ),7),0,1,2 ),
1,CASE( MOD ((TODAY()) - DATE( 1900,1,6 ),7),0,2,1 ),
IF(MOD((Planned_Start_Date__c) - DATE( 1900,1,6 ),7) - MOD((TODAY()) - DATE( 1900,1,6 ),7) &lt;= 0 ,0,
IF(MOD((TODAY()) - DATE( 1900,1,6 ),7) = 0,1,-2 ) ) )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Duration (In Business Days)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
